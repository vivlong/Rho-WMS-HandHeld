      <input class="form-control input-sm" id="GoodsReceiptNoteNo" type="text" placeholder="" disabled>
      <p></p>
      <label class="control-label small">Store No</label>
      <input class="form-control input-sm" id="StoreNo" type="text" placeholder="" onkeypress="GotoJump(event);">
    </div>
    <p></p>
    <div class="text-center">
      <button type="button" class="btn btn-default btn-sm" onclick="Rho.WebView.navigateBack();">Putaway</button> &nbsp;&nbsp;
