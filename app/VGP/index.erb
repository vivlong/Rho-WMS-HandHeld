<div data-role="page">
	<script src="/public/JsController/VGP_Index.js" type="text/javascript"></script>
	<div data-role="header" data-position="inline">
	  <h5 class="text-center">Verify GRN & Putaway</h5>
	</div>
  <div data-role="content">
    <div style="position:relative;">
      <div class="control-group">
        <label for="text_LoadCutomer" class="col-xs-3 control-label small">Cutomer</label>
        <div class="col-xs-8 SelectorAsInput">
          <select name="selector_LoadCutomer" id="selector_LoadCutomer" class="form-control input-sm SelectorAsInput_Selector" onChange="SelectCustomer();"></select>
          <input name="text_LoadCutomer" id="text_LoadCutomer" type="text" class="form-control input-sm SelectorAsInput_Input">
        </div>
      </div>
      <div class="control-group clearfix Margin-Top-p10">
        <label for="text_LoadGRN" class="col-xs-3 control-label small">GRN</label>
        <div class="col-xs-8 SelectorAsInput">
          <select name="selector_LoadGRN" id="selector_LoadGRN" class="form-control input-sm SelectorAsInput_Selector" onChange="SelectGRN();" onmousedown="LoadGRN();searchfield('text_LoadGRN',SelectorValue)"></select>
          <input name="text_LoadGRN" id="text_LoadGRN" type="text" class="form-control input-sm SelectorAsInput_Input">
        </div>
      </div>
    </div>
    <div class="clearfix center-block Margin-Top-p10">
      <table class="footable table table-bordered table-condensed xsmall Table-Text" id="GoodsReceipt">
        <thead>
          <tr>
            <th data-sort-initial="true">Customer</th>
            <th data-sort-initial="true">GRN</th>
            <th data-sort-initial="true">Rcp Date</th>
            <th data-sort-ignore="true">Ref No</th>
          </tr>
        </thead>
        <tbody id="GoodsReceipt_tbody"></tbody>
      </table>
    </div>
    <div class="text-right Margin-Top-m10"><button type="button" class="btn btn-default btn-sm" onclick="Rho.WebView.navigateBack();">Back</button></div>
  </div>
  
</div>